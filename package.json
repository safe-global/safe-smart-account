    "hardhat": "^2.0.8",
    "hardhat-deploy": "^0.7.0-beta.38",
    "prettier": "^2.1.2",
    "prettier-plugin-solidity": "^1.0.0-alpha.60",
    "solhint": "^3.3.2",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.13",
    "ts-node": "^9.1.1",
